<div id="<%= dom_id ticket %>">
  <p>
    <strong>Event:</strong>
    <%= ticket.event_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= ticket.user_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= ticket.quantity %>
  </p>

  <p>
    <strong>Purchased at:</strong>
    <%= ticket.purchased_at %>
  </p>

</div>
